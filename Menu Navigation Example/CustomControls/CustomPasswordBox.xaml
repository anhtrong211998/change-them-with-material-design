<UserControl x:Class="Menu_Navigation_Example.CustomControls.CustomPasswordBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Menu_Navigation_Example.CustomControls"
             xmlns:heplers="clr-namespace:Menu_Navigation_Example.Helpers"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel>
        <PasswordBox x:Name="txtPassword"
            Width="130" PasswordChanged="txtPassword_PasswordChanged"/>
    </StackPanel>
</UserControl>
